#################################################################
#    Object Properties
#################################################################

###  https://example.org/nepalplaces#connectsTo
:connectsTo rdf:type owl:ObjectProperty ;
                 rdfs:domain :Place ;
                 rdfs:range :Person ;
                 rdfs:comment "The 'connectsTo' property is used to establish a link between a place name and a person."@en ;
                 rdfs:label "connects to"@en .


###  https://example.org/nepalplaces#flowsIntoRiverAt
:flowsIntoRiverAt rdf:type owl:ObjectProperty ;
                       rdfs:domain :River ;
                       rdfs:range :Place ;
                       rdfs:comment "The 'flowsIntoRiverAt' property is used to define the place where a rivers flows into another river."@en ;
                       rdfs:label "flows into river at"@en .


###  https://example.org/nepalplaces#hasTributary
:hasTributary rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isTributaryOf ;
                   rdfs:domain :River ;
                   rdfs:range :River ;
                   rdfs:comment "The 'hasTributary' property indicates that the river has a tributary river."@en ;
                   rdfs:label "has tributary"@en .


###  https://example.org/nepalplaces#isCapitalOf
:isCapitalOf rdf:type owl:ObjectProperty ;
                  rdfs:domain :Settlement ;
                  rdfs:range :PopulatedPlace .


###  https://example.org/nepalplaces#isLocatedAt
:isLocatedAt rdf:type owl:ObjectProperty ;
                  rdfs:domain :Place ;
                  rdfs:range :Place ;
                  rdfs:comment "The 'isLocatedAt' property is used to indicate a place an individual is located At, e.g., at the bank of a particular river."@en ;
                  rdfs:label "is located at"@en .


###  https://example.org/nepalplaces#isLocatedIn
:isLocatedIn rdf:type owl:ObjectProperty ,
                           owl:TransitiveProperty ;
                  rdfs:domain :Place ;
                  rdfs:range :Place ;
                  rdfs:comment "The 'isLocatedIn' property is used to indicate a larger spatial area an individual is located in." ;
                  rdfs:label "is located in"@en .


###  https://example.org/nepalplaces#isTributaryOf
:isTributaryOf rdf:type owl:ObjectProperty ;
                    rdfs:domain :River ;
                    rdfs:range :River ;
                    rdfs:comment "The 'isTributaryOf' property indicates that one river is a tributary of another one." ;
                    rdfs:label "is tributary of"@en .


