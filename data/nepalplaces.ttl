# range bei Literalen: 	http://www.w3.org/2000/01/rdf-schema#Literal

<https://example.org/nepalplaces> rdf:type owl:Ontology ;
                                   cc:license <https://creativecommons.org/publicdomain/zero/1.0> ;
                                   dcterms:rights "CC-Zero" ;
                                   dcterms:issued "2021-08-03T11:18:00"^^xsd:dateTime ;
                                   owl:versionInfo 0.1 ;
                                   dcterms:creator "Sabine Tittel" ;
                                   vann:preferredNamespacePrefix "nplpl" ;
                                   dcterms:title "NepalPlaces"@en ;
                                   dcterms:description "This ontology is build to model place names and geographical names from Nepal that occur in the text corpus build by the research project &quot;Documents on the History of Religion and Law of Pre-modern Nepal&quot;, Heidelberg Academy of Sciences and Humanities. The corpus contains documents and other texts from the early Śāha (1769-1846) and Rāṇā (1846-1951) periods."@en ;
                                   vann:preferredNamespaceUri "https://example.org/nepalplaces#" .

