{
  "@context": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "gndo": "https://d-nb.info/standards/elementset/gnd#",
    "schema": "http://schema.org/",
    "viaf": "https://viaf.org/viaf/",
    "dbr": "http://dbpedia.org/resource/",
    "geonames": "https://www.geonames.org/",
    "wikidata": "https://www.wikidata.org/wiki/"
  },
  "@id": "https://example.org/nepalpeople",
  "mapping": [
    {
      "personProperty": "n",
      "ontologyProperty": "gndo:preferredNameEntityForThePerson"
    },
    {
      "personProperty": "gender",
      "ontologyProperty": "schema:gender"
    },
    {
      "personProperty": "anglicized_name",
      "ontologyProperty": "foaf:name"
    },
    {
      "personProperty": "surname",
      "ontologyProperty": "foaf:surname"
    },
    {
      "personProperty": "forename",
      "ontologyProperty": "foaf:forename"
    }
  ]
}
