###  https://example.org/nepalpeople#hasPosition
:hasPosition rdf:type owl:ObjectProperty ;
                  rdfs:comment "The 'hasPosition' property specifies a position, i.e. an office, title, role, that is held by a person."@en ;
                  rdfs:label "has position"@en ;
                  rdfs:domain :Person ;
                  rdfs:range :Position .


###  https://example.org/nepalpeople#hasSuccessor
:hasSuccessor rdf:type owl:ObjectProperty ;
              owl:inverseOf :succeeds ;
              rdfs:domain :Person ;
              rdfs:range :Person ;
              rdfs:comment "The 'hasSuccessor' property specifies a successor (in office, in title, etc.)."@en ;
              rdfs:label "has successor"@en .


###  https://d-nb.info/standards/elementset/agrelon#hasSpouse
agrelon:hasSpouse rdf:type owl:ObjectProperty ;
              rdfs:isDefinedBy <https://d-nb.info/standards/elementset/agrelon#hasSpouse> ;
              rdfs:subPropertyOf :hasSexualPartner ;
              rdf:type owl:SymmetricProperty ;
              rdfs:domain :Person ;
              rdfs:range :Person ;
              rdfs:label "has spouse"@en .


###  https://example.org/nepalpeople#hasHouseholdConcubine
:hasHouseholdConcubine rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasSexualPartner ;
              rdfs:domain :Person ;
              rdfs:range :Person ;
              rdfs:comment "The 'hasHouseholdConcubine' property specifies the sexual partner living within the household."@en ;
              rdfs:label "has household concubine"@en .


###  https://example.org/nepalpeople#hasNonHouseholdConcubine
:hasNonHouseholdConcubine rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasSexualPartner ;
              rdfs:domain :Person ;
              rdfs:range :Person ;
              rdfs:comment "The 'hasNonHouseholdConcubine' property specifies the sexual partner not living within the household."@en ;
              rdfs:label "has non household concubine"@en .


### http://www.w3.org/2006/time#hasTRS
time:hasTRS rdf:type owl:ObjectProperty ;
              rdfs:comment """A temporal reference system, such as a temporal coordinate system (with an origin, direction, and scale), a calendar-clock combination, or a (possibly hierarchical) ordinal system."""@en ;
              rdfs:isDefinedBy <http://www.w3.org/2006/time#hasTRS> .


###  https://d-nb.info/standards/elementset/agrelon#hasVitalLethalContact
agrelon:hasVitalLethalContact rdf:type owl:ObjectProperty ;
              rdfs:isDefinedBy <https://d-nb.info/standards/elementset/agrelon#hasVitalLethalContact> ;
              rdf:type owl:SymmetricProperty ;
              rdfs:domain :Person ;
              rdfs:range :Person ;
              rdfs:label "has vital or lethal contact"@en .


### http://www.w3.org/2006/time#inDateTime
time:inDateTime rdf:type owl:ObjectProperty ;
              rdfs:isDefinedBy <http://www.w3.org/2006/time#inDateTime> .


###  https://example.org/nepalpeople#inducedBy
:inducedBy rdf:type owl:ObjectProperty ;
              rdfs:domain :Event ;
              rdfs:range :Person , :Event ;
              rdfs:comment "The 'inducedBy' property specifies an event that has been induced, caused or motivated by a person or an event."@en ;
              rdfs:label "induced by"@en .


###  https://example.org/nepalpeople#induces
:induces rdf:type owl:ObjectProperty ;
              rdfs:domain :Person , :Event ;
              rdfs:range :Event ;
              rdfs:comment "The 'induces' property specifies a person or an event who / that induces, causes or motivates an event."@en ;
              rdfs:label "induces"@en .


###  https://d-nb.info/standards/elementset/gnd#preferredNameEntityForThePerson
gnd:preferredNameEntityForThePerson rdf:type owl:ObjectProperty ;
              rdfs:isDefinedBy <https://d-nb.info/standards/elementset/gnd#preferredNameEntityForThePerson> ;
              rdfs:domain gnd:DifferentiatedPerson ;
              rdfs:range gnd:NameOfThePerson .


###  https://example.org/nepalpeople#hasPlace
:hasPlace rdf:type owl:ObjectProperty ;
              rdfs:label "has place"@en ;
              rdfs:domain :Position , :Event ;
              rdfs:range :Place ;
              rdfs:comment "The 'hasPlace' property specifies the place of a position or an event."@en .


###  https://example.org/nepalpeople#hasTime
:hasTime rdf:type owl:ObjectProperty ;
              rdfs:label "has time"@en ;
              rdfs:domain :Position , :Event ;
              rdfs:range time:TemporalEntity ;
              rdfs:comment "The 'hasTime' property specifies the time of a position."@en .


###  https://example.org/nepalpeople#resultsIn
:resultsIn rdf:type owl:ObjectProperty ;
              rdfs:domain :Event ;
              rdfs:range :Place ;
              rdfs:comment "The 'resultsIn' property specifies an event whose results in a place (building and sim.)."@en ;
              rdfs:label "results in"@en .


###  https://example.org/nepalpeople#succeeds
:succeeds rdf:type owl:ObjectProperty ;
          rdfs:comment "The 'succeeds' property specifies a person succeeded  (in office, in title, etc.)."@en ;
          owl:inverseOf :hasSuccessor ;
          rdfs:domain :Person ;
          rdfs:range :Person ;
          rdfs:label "succeeds"@en .


###  https://example.org/nepalpeople#target
:target rdf:type owl:ObjectProperty ;
              rdfs:domain :Position ;
              rdfs:range :Position ,
                         :Person ;
              rdfs:comment "The 'target' property specifies a person or a group of people supervised, overseen, and sim., connected to a position, e.g., an office."@en ;
              rdfs:label "target"@en .


###  https://example.org/nepalpeople#targetGroup
:targetGroup rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :target ;
              rdfs:domain :Position ;
              rdfs:range :Position ;
              rdfs:comment "The 'targetGroup' property specifies the group of people (as holders of a title, an office, or a role) supervised, overseen, and sim."@en ;
              rdfs:label "target person"@en .


###  https://example.org/nepalpeople#targetPerson
:targetPerson rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :target ;
              rdfs:domain :Position ;
              rdfs:range :Person ;
              rdfs:comment "The 'targetPerson' property specifies the person concerned, e.g., married, supervised, overseen, and sim."@en ;
              rdfs:label "target person"@en .


###  https://d-nb.info/standards/elementset/gnd#variantNameEntityForThePerson
gnd:variantNameEntityForThePerson rdf:type owl:ObjectProperty ;
              rdfs:isDefinedBy <https://d-nb.info/standards/elementset/gnd#variantNameEntityForThePerson> ;
              rdfs:domain gnd:DifferentiatedPerson ;
              rdfs:range gnd:NameOfThePerson .


### http://www.w3.org/2006/time#year
time:year rdf:type owl:ObjectProperty ;
              rdfs:isDefinedBy <http://www.w3.org/2006/time#year> .


#################################################################
#    Classes
#################################################################

###  http://purl.org/vocab/bio/0.1/Adoption
bio:Adoption rdf:type owl:Class ;
          rdfs:subClassOf bio:Event ;
          rdfs:isDefinedBy <http://purl.org/vocab/bio/0.1/Adoption> ;
          rdfs:comment "The event of creating of a legal parent/child relationship that does not exist biologically."@en ;
          rdfs:label "adoption"@en .


###  http://purl.org/vocab/bio/0.1/Birth
bio:Birth rdf:type owl:Class ;
          rdfs:subClassOf bio:Event ;
          rdfs:isDefinedBy <http://purl.org/vocab/bio/0.1/Birth> ;
          rdfs:comment "The event of a person entering into life."@en ;
          rdfs:label "birth"@en .


###  https://www.w3.org/2006/time#DateTimeDescription
time:DateTimeDescription rdf:type owl:Class ;
          rdfs:isDefinedBy <http://www.w3.org/2006/time#DateTimeDescription> ;
          rdfs:comment "Description of date and time structured with separate values for the various elements of a calendar-clock system. The temporal reference system is fixed to Gregorian Calendar, and the range of year, month, day properties restricted to corresponding XML Schema types xsd:gYear, xsd:gMonth and xsd:gDay, respectively."@en ;
          rdfs:label "Date-Time description"@en .


###  http://purl.org/vocab/bio/0.1/Death
bio:Death rdf:type owl:Class ;
          rdfs:subClassOf bio:Event ;
          rdfs:isDefinedBy <http://purl.org/vocab/bio/0.1/Death> ;
