  <rdf:Description rdf:about="https://nepalica.hadw-bw.de/nepal/editions/show/431">
    <rdf:type rdf:resource="http://purl.org/dc/terms/Document"/>
    <rdf:type rdf:resource="http://www.w3.org/ns/lemon/lime#Lexicon"/>
    <dct:description xml:lang="en">This is a text edition of the project 'Documents on the History of Religion and Law of Pre-modern Nepal', Heidelberg Academy of Sciences and Humanities, https://www.hadw-bw.de/nepal.</dct:description>
    <dct:language rdf:resource="http://lexvo.org/id/iso639-3/ne"/>
    <dct:language rdf:resource="http://lexvo.org/id/iso639-3/en"/>
    <dct:identifier xml:lang="en">DNA_0001_0021</dct:identifier>
    <dct:title xml:lang="en">An arjī from Jasapāu Thāpā re Raṇajit Siṃ's failure to gain victory (VS 1871)</dct:title>
    <dct:type xml:lang="en">ciṭṭhīpatra, arjī; unspecified</dct:type>
    <dct:abstract xml:lang="en">It is the letter of Jaspāu Thāpā from Arkī to authority concerned stating about the war in which Raṇvīra Simha was unable to get victory.</dct:abstract>
    <dct:date xml:lang="en">VS 1871 Śrāvaṇa bright 6 Friday</dct:date>
    <lime:language xml:lang="en">unspecified; Devanagari</lime:language>
    <dct:format xml:lang="en">26; 16.5; cm</dct:format>
    <dct:extent xml:lang="en">damaged by breaking; complete</dct:extent>
    <dct:publisher xml:lang="en">National Archives Kathmandu; Ms. no. 21</dct:publisher>
    <dct:source xml:lang="en">NGMPP catalogue card; Microfilmed on 22/02/2000 as NGMPP DNA 1/21</dct:source>
    <dct:hasPart xml:lang="en">21</dct:hasPart>
    <schema:created xml:lang="en">10/22/18, 9:37 AM</schema:created>
  </rdf:Description>
