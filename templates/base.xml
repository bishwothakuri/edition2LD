<?xml version="1.0" encoding="UTF-8"?>
<TEI xml:id="{{id}}" xmlns="http://www.tei-c.org/ns/1.0">
    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title type="main">{{title_main}}</title>
                <title type="short">{{title_short}}</title>
                <title type="sub">{{title_sub}}</title>
                <author role="issuer">{{author_role_issuer}}</author>

                <respStmt>
                    <resp>main editor and translator</resp>
                    <name type="main_editor">{{main_editor}}</name>
                </respStmt>
            </titleStmt>
            <physDesc>
                <p>For details see <ref target="{{physDesc_ref_target}}">entry in database</ref>
                </p>
            </physDesc>
        </fileDesc>
    </teiHeader>
    <text>
        <body>
            <div xml:id="et" type="english_translation" corresp="#ed" xml:lang="eng">
                <list type="person"></list>
                <list type="place"></list>
                <list type="gloss"></list>
            </div>
        </body>
    </text>
</TEI>
